<div class="block">
  
  <%= render :partial => 'secondary_navigation', :locals => { :klass => 'list' } %>
            
  <div class="content">          
    <h2 class="title">All Tenants</h2>
    <div class="inner">
      <table class="table">
        <tr>             
          <th>First Name</th>
          <th>Last Name</th>
          <th class="last">&nbsp;</th>
        </tr>
        <% @tenants.each do |tenant| -%>
          <tr class="<%= cycle("odd", "even") %>">
            <td><%= tenant.first_name %></td>
            <td><%= tenant.second_name %></td>
            <td class="last">
              <%= link_to "Show", tenant_path(tenant) %> |
              <%= link_to "Edit", edit_tenant_path(tenant) %> |
              <%= link_to "Delete", tenant_path(tenant), :method => :delete, :confirm => "Are you sure?" %>
            </td>
          </tr>          
        <% end -%>
      </table>
      <div class="actions-bar wat-cf">
        <div class="actions"></div>
        <%#= will_paginate @tenants  %>
      </div>
    </div>
  </div>
  
</div>

<% content_for :sidebar, render(:partial => 'shared/sidebar') -%>
