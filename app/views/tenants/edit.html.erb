<div class="block">
  <%= render :partial => 'secondary_navigation', :locals => { :klass => 'edit', :tenant => @tenant } %>
  <div class="content">            
    <h2 class="title">Edit Tenant</h2>
    <div class="inner">
      <%= form_for @tenant, :url => tenant_path(@tenant), :html => { :class => :form } do |f| -%>
        <%= render :partial => "form", :locals => {:f => f} %>
      <% end -%>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'shared/sidebar') -%>